{
  "name": "mdjs",
  "version": "0.1.10",
  "description": "一个纯洁的md在线浏览",
  "main": "lib/main.js",
  "scripts": {
    "check": "fecs check src/ test/",
    "compile": "babel src/ -d lib/",
    "watch": "npm run compile -- --watch",
    "start": "node test.js",
    "prepublish": "npm run compile",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-register -t 5000 --recursive  -R spec test/",
    "test": "mocha --compilers js:babel-register --reporter spec --timeout 5000 --recursive test/",
    "release": "bash release.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xuexb/mdjs.git"
  },
  "author": "xiaowu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xuexb/mdjs/issues"
  },
  "homepage": "https://github.com/xuexb/mdjs/",
  "dependencies": {
    "art-template": "^3.0.3",
    "babel-runtime": "^5.8.20",
    "express": "^4.13.3",
    "fs-grep": "*",
    "highlight.js": "^8.9.1",
    "key-cache": "0.x.x",
    "marked": "^0.3.5",
    "serve-static": "^1.10.0",
    "transliteration": "^0.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.x",
    "babel-plugin-add-module-exports": "*",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-stage-0": "6.x",
    "babel-preset-stage-3": "6.x",
    "babel-register": ">=6.7.2",
    "fecs": ">=0.6.5",
    "istanbul": ">=1.0.0-alpha.2",
    "mocha": ">=2.4.5",
    "prepush": ">=3.1.8",
    "should": ">=8.2.2"
  },
  "keywords": [
    "markdown",
    "marked",
    "md",
    "markdown-web"
  ],
  "engines": {
    "node": ">= 0.12.0"
  }
}
